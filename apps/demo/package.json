{
    "name": "demo",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "tsc -b && vite build",
        "dev": "vite",
        "lint:eslint": "eslint --cache --max-warnings 60 .",
        "lint:eslint:config-check": "eslint-config-prettier src/index.tsx",
        "lint:eslint:fix": "eslint --cache --max-warnings 60 --fix .",
        "lint:knip": "knip --max-issues 100",
        "lint:knip:fix": "knip --fix",
        "lint:prettier": "prettier . --check",
        "lint:prettier:fix": "prettier . --write",
        "prepare": "husky",
        "preview": "vite preview"
    },
    "dependencies": {
        "@monolab/is-even": "workspace:*",
        "@monolab/is-odd": "workspace:*",
        "solid-js": "1.9.5"
    },
    "devDependencies": {
        "typescript": "~5.7.2",
        "vite": "^6.2.0",
        "vite-plugin-solid": "^2.11.2"
    }
