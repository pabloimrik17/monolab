{
    "name": "@monolab/demo",
    "version": "0.0.2",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "tsc -b && vite build",
        "dev": "vite",
        "lint:eslint": "eslint --cache --max-warnings 60 .",
        "lint:eslint:config-check": "eslint-config-prettier src/index.tsx",
        "lint:eslint:fix": "eslint --cache --max-warnings 60 --fix .",
        "lint:knip": "knip --max-issues 100",
        "lint:knip:fix": "knip --fix",
        "lint:prettier": "prettier . --check",
        "lint:prettier:fix": "prettier . --write",
        "preview": "vite preview"
    },
    "dependencies": {
        "@monolab/is-even": "workspace:*",
        "@monolab/is-odd": "workspace:*",
        "solid-js": "1.9.9"
    },
    "devDependencies": {
        "typescript": "5.9.3",
        "vite": "6.4.1",
        "vite-plugin-solid": "2.11.8"
    },
    "packageManager": "pnpm@10.19.0",
    "engines": {
        "node": "22.21.0",
        "pnpm": "10.19.0"
    }
}
