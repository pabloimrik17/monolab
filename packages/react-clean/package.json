{
    "name": "@monolab/react-clean",
    "version": "0.3.4",
    "private": false,
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./package.json": "./package.json"
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "package.json",
        "README.md",
        "CHANGELOG.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "pnpm run build:typecheck && pnpm run build:lib",
        "build:check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
        "build:lib": "tsdown",
        "build:typecheck": "tsc --noEmit",
        "lint:eslint": "eslint --cache --max-warnings 60 .",
        "lint:eslint:config-check": "eslint-config-prettier src/index.ts",
        "lint:eslint:fix": "eslint --cache --max-warnings 60 --fix .",
        "lint:knip": "knip --max-issues 100",
        "lint:knip:fix": "knip --fix",
        "lint:prettier": "prettier . --check",
        "lint:prettier:fix": "prettier . --write",
        "test:mutation": "stryker run",
        "test:mutation:report": "open reports/mutation/index.html",
        "test:unit": "vitest run",
        "test:unit:coverage": "vitest run --coverage",
        "test:unit:watch": "vitest"
    },
    "dependencies": {
        "@monolab/react-hooks": "workspace:*",
        "tslib": "^2.3.0"
    },
    "devDependencies": {
        "@arethetypeswrong/cli": "0.18.2",
        "@testing-library/react": "catalog:",
        "@types/react": "18.3.26",
        "inversify": "^7.8.1",
        "jsdom": "catalog:",
        "react": "^18.0.0",
        "react-dom": "^18.3.1",
        "rxjs": "^7.0.0",
        "tsdown": "0.15.9",
        "typescript": "5.9.3",
        "vitest": "catalog:"
    },
    "peerDependencies": {
        "inversify": ">=7.0.0",
        "react": ">=18.0.0",
        "rxjs": ">=7.0.0"
    },
    "packageManager": "pnpm@10.19.0",
    "engines": {
        "node": "22.21.0",
        "pnpm": "10.19.0"
    }
}
