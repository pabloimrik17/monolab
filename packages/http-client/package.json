{
    "name": "@m0n0lab/http-client",
    "version": "0.1.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/pabloimrik17/monolab.git",
        "directory": "packages/http-client"
    },
    "author": "Pablo F.G.",
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "package.json",
        "README.md",
        "CHANGELOG.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsc -b",
        "lint:eslint": "eslint --cache --max-warnings 40 .",
        "lint:eslint:config-check": "eslint-config-prettier src/index.ts",
        "lint:eslint:fix": "eslint --cache --max-warnings 40 --fix .",
        "lint:knip": "knip --max-issues 70",
        "lint:knip:fix": "knip --fix",
        "lint:prettier": "prettier . --check",
        "lint:prettier:fix": "prettier . --write",
        "test:integration": "vitest run **/*.integration.ts",
        "test:integration:watch": "vitest **/*.integration.ts",
        "test:mutation": "stryker run",
        "test:mutation:report": "open reports/mutation/index.html",
        "test:types": "vitest --typecheck.enabled --run",
        "test:types:watch": "vitest --typecheck.enabled",
        "test:ui": "vitest --ui",
        "test:unit": "vitest run",
        "test:unit:coverage": "vitest run --coverage",
        "test:unit:watch": "vitest",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "tslib": "2.8.1"
    },
    "peerDependencies": {
        "axios": "^1.6.0"
    },
    "peerDependenciesMeta": {
        "axios": {
            "optional": false
        }
    },
    "devDependencies": {
        "axios": "^1.8.1",
        "axios-mock-adapter": "^2.1.0",
        "typescript": "5.9.3",
        "vitest": "catalog:"
    },
    "packageManager": "pnpm@10.19.0",
    "engines": {
        "node": "^20.18.0 || >=22.0.0",
        "pnpm": "10.19.0"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true,
        "registry": "https://registry.npmjs.org/"
    }
}
