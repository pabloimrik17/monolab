{
    "name": "@monolab/source",
    "version": "0.0.0",
    "private": true,
    "license": "MIT",
    "type": "module",
    "scripts": {
        "demo:dev": "nx run @monolab/demo:dev",
        "jsr:sync-version": "node tools/sync-jsr-version.js",
        "lint:knip": "knip --max-issues 70",
        "lint:knip:fix": "knip --fix",
        "monorepo:clean:dist": "pnpm dlx rimraf@5.0.10 -g \"**/dist\"",
        "monorepo:clean:eslintcache": "pnpm dlx rimraf@5.0.10 -g \"**/.eslintcache\"",
        "monorepo:clean:hard": "pnpm run monorepo:clean:soft && pnpm run monorepo:clean:dist && pnpm run monorepo:clean:node_modules",
        "monorepo:clean:node_modules": "pnpm dlx rimraf@5.0.10 -g \"**/node_modules\"",
        "monorepo:clean:nx": "nx reset",
        "monorepo:clean:soft": "pnpm run monorepo:clean:tsbuildinfo && pnpm run monorepo:clean:eslintcache && pnpm run monorepo:clean:nx",
        "monorepo:clean:tsbuildinfo": "pnpm dlx rimraf@5.0.10 -g \"**/tsconfig.tsbuildinfo\"",
        "prepare": "husky",
        "react-clean:release": "nx release --skip-publish --projects @monolab/react-clean && pnpm run jsr:sync-version",
        "react-clean:release:dry": "nx release --skip-publish --dry-run --projects @monolab/react-clean",
        "react-hooks:release": "nx release --skip-publish --projects @monolab/react-hooks && pnpm run jsr:sync-version",
        "react-hooks:release:dry": "nx release --skip-publish --dry-run --projects @monolab/react-hooks"
    },
    "devDependencies": {
        "@codecov/bundle-analyzer": "^1.9.1",
        "@commitlint/cli": "^19.8.0",
        "@commitlint/config-conventional": "^19.8.0",
        "@commitlint/types": "^19.8.0",
        "@eslint/js": "^9.25.0",
        "@nx/js": "21.3.11",
        "@stryker-mutator/core": "^9.3.0",
        "@stryker-mutator/typescript-checker": "^9.3.0",
        "@stryker-mutator/vitest-runner": "^9.3.0",
        "@swc-node/register": "~1.9.1",
        "@swc/core": "~1.5.7",
        "@swc/helpers": "~0.5.11",
        "@types/node": "^22.10.5",
        "@vitest/coverage-v8": "catalog:",
        "@vitest/ui": "catalog:",
        "eslint": "^9.25.0",
        "eslint-config-prettier": "^10.1.2",
        "husky": "^9.1.7",
        "knip": "^5.50.5",
        "lint-staged": "16.2.6",
        "markdownlint-cli": "^0.44.0",
        "nx": "21.3.11",
        "prettier": "^2.6.2",
        "prettier-plugin-organize-imports": "^4.2.0",
        "prettier-plugin-packagejson": "^2.5.10",
        "stylelint": "^16.25.0",
        "stylelint-config-standard": "^38.0.0",
        "tslib": "^2.3.0",
        "typescript": "5.9.3",
        "typescript-eslint": "^8.39.0",
        "validate-branch-name": "^1.3.2",
        "vite-tsconfig-paths": "catalog:",
        "vitest": "catalog:"
    },
    "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
    "engines": {
        "node": "22.17.0",
        "pnpm": "10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad"
    }
}
